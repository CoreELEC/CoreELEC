# OS_MAJOR: OS major Version
  OS_MAJOR="21"

# OS_MINOR: OS minor Version
  OS_MINOR="1"

# OS_PATCH: OS patch Version
  OS_PATCH="1"

# OS_VERSION: OS Version
  OS_VERSION="${OS_MAJOR}.${OS_MINOR}.${OS_PATCH}"

# OS_CODENAME: OS Codename
  OS_CODENAME="Omega"

# OS_STAGE: OS Release Stage (alpha, beta, rc, build)
  OS_STAGE=""

# BUILD_NUM: Build number
  BUILD_NUM=""

# ADDON_VERSION: Addon version
  ADDON_VERSION="${OS_MAJOR}.${OS_MINOR}.1"
